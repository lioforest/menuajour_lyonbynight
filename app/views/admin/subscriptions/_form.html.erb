<%= form_for([:admin, @admin_subscription]) do |f| %>
  <% if @admin_subscription.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@admin_subscription.errors.count, "error") %> impossible d'enregistrer cet utilisateur:</h2>

      <ul>
      <% @admin_subscription.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>


  <div class="form-group">
    <%= f.label :nom %>
    <%= f.text_area :name, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :DAte %>
    <%= f.text_area :start_date, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :DAte %>
    <%= f.text_area :end_date, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :prix %>
    <%= f.text_area :price, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :utilisateur %>
    <%= f.text_area :user_id, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.submit  class: 'btn btn-default' %>
  </div>
<% end %>
